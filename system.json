{
  "id": "tsa",
  "title": "Twenty Sided Adventures",
  "description": "A system for playing Twenty Sided Adventures in the Foundry Virtual Tabletop environment.",
  "version": "2.4.0",
  "compatibility": {
    "minimum": "10.303",
    "verified": "11"
  },
  "authors": [
    {
      "name": "KCMedia",
      "flags": {}
    }
  ],
  "esmodules": [
    "tsa.mjs"
  ],
  "styles": [
    "tsa.css"
  ],
  "packs": [
    {
      "name": "heroes",
      "label": "Starter Heroes",
      "system": "tsa",
      "path": "packs/heroes.db",
      "type": "Actor",
      "private": false,
      "flags": {}
    },
    {
      "name": "npcs",
      "label": "NPCs",
      "system": "tsa",
      "path": "packs/npcs.db",
      "type": "Actor",
      "private": false,
      "flags": {}
    },
    {
      "name": "items",
      "label": "Items",
      "system": "tsa",
      "path": "packs/items.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "tradegoods",
      "label": "Trade Goods",
      "system": "tsa",
      "path": "packs/tradegoods.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "spells",
      "label": "Spells",
      "system": "tsa",
      "path": "packs/spells.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
     {
      "name": "powers",
      "label": "Powers",
      "system": "tsa",
      "path": "packs/powers.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "backgrounds",
      "label": "Backgrounds",
      "system": "tsa",
      "path": "packs/backgrounds.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "classes",
      "label": "Classes",
      "system": "tsa",
      "path": "packs/classes.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "subclasses",
      "label": "Subclasses",
      "system": "tsa",
      "path": "packs/subclasses.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "classfeatures",
      "label": "Class Features",
      "system": "tsa",
      "path": "packs/classfeatures.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "ancestries",
      "label": "Ancestries",
      "system": "tsa",
      "path": "packs/ancestries.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "npcfeatures",
      "label": "NPC Features",
      "system": "tsa",
      "path": "packs/npcfeatures.db",
      "type": "Item",
      "private": false,
      "flags": {}
    },
    {
      "name": "rules",
      "label": "Rules",
      "system": "tsa",
      "path": "packs/rules.db",
      "type": "JournalEntry",
      "private": false,
      "flags": {}
    },
    {
      "name": "tables",
      "label": "Tables",
      "system": "tsa",
      "path": "packs/tables.db",
      "type": "RollTable",
      "private": false,
      "flags": {}
    }
  ],
  "packFolders": [
    {
      "name": "Core Content",
      "sorting": "m",
      "color": "#cd2c1e",
      "packs": [
        "rules",
        "tables",
        "heroes"
      ],
      "folders": [
        {
          "name": "Character Features",
          "sorting": "m",
          "color": "#b72c1f",
          "packs": [
            "classes",
            "subclasses",
            "classfeatures",
            "backgrounds",
            "races"
          ]
        },
        {
          "name": "Items, Powers, & Spells",
          "sorting": "m",
          "color": "#941919 ",
          "packs": [
            "items",
            "powers",
            "spells",
            "tradegoods"
          ]
        },
        {
          "name": "NPCs",
          "sorting": "m",
          "color": "#7d160c",
          "packs": [
            "npcs",
            "npcfeatures"
          ]
        }
      ]
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json",
      "flags": {}
    }
  ],
  "media": [
    {
      "type": "cover",
      "loop": false,
      "flags": {}
    }
  ],
  "socket": true,
  "gridDistance": 1,
  "gridUnits": "m",
  "primaryTokenAttribute": "attributes.hp",
  "flags": {
    "needsMigrationVersion": "2.4.0",
    "compatibleMigrationVersion": "0.8",
    "hotReload": {
      "extensions": ["css", "hbs", "json"],
      "paths": ["tsa.css", "templates", "lang"]
    }
  }
}
